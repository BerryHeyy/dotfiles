{
    "vscord.status.showElapsedTime": false,
    "vscord.status.details.text.editing": "In {workspace}",
    "vscord.status.details.text.viewing": "In {workspace}",
    "vscord.status.state.text.editing": "Working on {file_name}{file_extension}",
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "editor.fontFamily": "FiraCode Nerd Font Mono",
    "editor.fontSize": 16,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "vscord.status.idle.check": false,
    "vscord.status.idle.enabled": false,
    "editor.lineNumbers": "relative",
    "cmake.showOptionsMovedNotification": false,
    "window.menuBarVisibility": "hidden",
    "cmake.configureOnOpen": true,
    "editor.wordWrap": "on",
    "editor.rulers": [
        120
    ],
    "vscord.status.buttons.button1.git.active.enabled": false,
    "vscord.status.buttons.button1.git.idle.enabled": false,
    "vscord.status.buttons.button1.git.inactive.enabled": false
}
